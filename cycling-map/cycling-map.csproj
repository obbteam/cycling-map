<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net8.0-windows</TargetFramework>
        <RootNamespace>cycling_map</RootNamespace>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <UseWPF>true</UseWPF>
    </PropertyGroup>

    <ItemGroup>
      <None Remove="images\bicycle.png" />
      <Resource Include="images\bicycle.png" />
      <Resource Include="images\car.png" />
      <None Remove="images\pedestrian.png" />
      <Resource Include="images\pedestrian.png" />
    </ItemGroup>

</Project>
